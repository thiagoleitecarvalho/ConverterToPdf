<FindBugsFilter>
    <!--Quebra a build caso seja detectada vulnerabilidade de seguranca -->
    <Match>
        <Bug category="SECURITY" />
    </Match>
    <!--Quebra a build caso seja detectada possibilidade de injecao maliciosa de codigo, caso seja de severidade alta -->
    <Match>
        <Bug category="MALICIOUS_CODE" />
        <Priority value="2" />
    </Match>
</FindBugsFilter>